(realize-tcb (pathPlanner ?x ?y => ?waypoints ?number_of_waypoints)
	(module "NavigationModule2")
	(action (	
    (format t "===== pathPlanner~%")	
    (format t "pathPlanner: name Goal ~s ~s ~%" '?x '?y)
    (let 
      ((answer (tcl-query :server 'plannercoord  
                          :service 'plannerquery
                          :request '(?x ?y))))
      (tcl-bind-var :name '?waypoints :value (first answer))
      (tcl-bind-var :name '?number_of_waypoints :value (second answer))
    )
    '(SUCCESS (OK)))
  )
)